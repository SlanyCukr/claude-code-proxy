# Claude Code Proxy Configuration
# Copy this file to .env and fill in your values
# Note: The proxy primarily uses ~/.config/claude-code-proxy/config.json
# These environment variables can override config file settings

# z.ai API key for subagent routing
ZAI_API_KEY=

# Proxy settings
PROXY_PORT=8080
PROXY_DEBUG=true

# Anthropic settings
ANTHROPIC_BASE_URL=https://api.anthropic.com

# z.ai settings
ZAI_BASE_URL=https://api.z.ai/api/anthropic
